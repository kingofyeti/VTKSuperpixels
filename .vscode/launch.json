    "version": "0.2.0",
    "configurations": [

        {
            "name": "(gdb) Launch",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build/VTKSuperpixels",
            "args": ["../TestData/volumeTest.nii", "500"],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": true,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ],
            "args": ["../TestData/Brats18_CBICA_ABT_1_t1ce.nii.gz ../TestData/Brats18_CBICA_ABT_1_t1cea.nii.gz 1000"]
        }
    ]
}